@model IEnumerable<AlutaApp.Models.User>

<table id="datatable" class="table table-theme table-row v-middle" data-plugin="dataTable">
        <thead>
            <tr>
                <th><span class="text-muted">FullName</span></th>
                <th><span class="text-muted">Institution</span></th>
                <th><span class="text-muted">Department</span></th>
                <th><span class="text-muted">Admission Year</span></th>
                <th><span class="text-muted">Grade Point</span></th>
                <th><span class="text-muted">Mobile Number</span></th>
                <th><span class="text-muted">Email</span></th>
                <th><span class="text-muted">Online</span></th>
                                           
                                            
                <th></th>
            </tr>
        </thead>
        <tbody id="search-results">
            @foreach (var item in Model) {

            <tr class="" data-id="16">
                <td >
                    <small class="text-muted">@item.FullName</small>
                </td>
                <td >
                                                
                    <small class="text-muted">@item.Institution.Name</small>
                </td>

                    <td >
                    <small class="text-muted">@item.Department.Name</small>
                </td>
                                            

                    <td >
                    <small class="text-muted">@item.YearOfAdmission</small>
                </td>
                    <td >
                    <small class="text-muted">@item.GradePoint</small>
                </td>
                    <td >
                    <small class="text-muted">@item.PhoneNumber</small>
                </td>
                    <td >
                    <small class="text-muted">@item.Email</small>
                </td> 
                <td >
                    <small class="text-muted">@item.Online</small>
                </td>                       
                                            
                <td>
                    <div class="item-action dropdown">
                        <a href="#" data-toggle="dropdown" class="text-muted">
                            <i data-feather="more-vertical"></i>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right bg-black" role="menu">
                            <a class="dropdown-item" href="../../Administrators/ViewAccount/@item.Id" id="viewInstitution">
                                View
                            </a>
                            <a class="dropdown-item edit" href="../../Administrators/EditUser/@item.Id" id="editInstitution">
                                Update
                            </a>
                                                       
                            <a class="dropdown-item edit" href="../../Administrators/DeleteUser/@item.Id" id="editInstitution">
                                Delete  
                            </a>
                        </div>
                    </div>
                </td>
            </tr>
        }
        </tbody>

    </table>
  <div>
                         
 </div> 


                              