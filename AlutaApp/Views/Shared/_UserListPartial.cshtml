@model IEnumerable<AlutaApp.Models.User>

<table id="datatable" class="table table-theme table-bordered table-striped table-row v-middle" data-plugin="dataTable">
        <thead>
            <tr>
                <th><span class="text-muted">FullName</span></th>
                <th><span class="text-muted">Institution</span></th>
                <th><span class="text-muted">Department</span></th>
                <th><span class="text-muted">Admission Year</span></th>
                <th><span class="text-muted">Grade Point</span></th>
                
                <th><span class="text-muted">Online</span></th>
                                           
                  <th><span class="text-muted">Action</span></th>                          
                
            </tr>
        </thead>
        <tbody id="search-results">
            @foreach (var item in Model) {

            <tr class="" data-id="16">
                <td >
                    @item.FullName
                </td>
                <td >
                                                
                    @item.Institution.Name
                </td>

                    <td >
                    @item.Department.Name
                </td>
                                            

                    <td >
                    @item.YearOfAdmission
                </td>
                    <td >
                    @item.GradePoint
                </td>
                 
                <td >
                    @item.Online
                </td>                       
                                            
                <td>
                  
                    <div class="btn-group">
                        <i class="fas fa-ellipsis-v text-center cursor" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                       
                        </i>
                   
                        <div class="dropdown-menu shadow animated--grow-in">
                            <a style='margin-right:16px' href="../../Administrators/ViewAccount/@item.Id" class="dropdown-item">
                                <i class="fas fa-eye text-success"></i> View
                            </a>
                            <a style='margin-right:16px' href="../../Administrators/EditUser/@item.Id" class="dropdown-item">
                                <i class="fas fa-edit text-warning"></i> Edit
                            </a>
                             <a style='margin-right:16px' href="../../Administrators/DeleteUser/@item.Id" class="dropdown-item">
                                <i class="fas fa-trash text-danger"></i> Delete
                            </a>
                        </div>
                    </div>
                </td>
            </tr>
        }
        </tbody>

    </table>
  <div>
                         
 </div> 


                              