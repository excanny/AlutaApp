@model IEnumerable<AlutaApp.Models.TimeTable>

<table id="datatable" class="table table-theme table-row v-middle" data-plugin="dataTable">
                                    <thead>
                                        <tr>
                                            <th><span class="text-muted">Course</span></th>
                                            <th><span class="text-muted">Day</span></th>
                                            <th><span class="text-muted">Hour</span></th>
                                            <th><span class="text-muted">Minute</span></th>
                                            <th><span class="text-muted">RemindInMinutes</span></th>
                                            <th><span class="text-muted">User</span></th>
                                            
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model) {

                                        <tr class="" data-id="16">
                                            <td >
                                                <small class="text-muted">@item.Course</small>
                                            </td>
                                            <td >
                                                <small class="text-muted">@item.Day</small>
                                            </td>
                                             
                                            <td >
                                                <small class="text-muted">@item.Hour</small>
                                            </td>


                                            <td >
                                                <small class="text-muted">@item.Minute</small>
                                            </td>

                                            <td >
                                                <small class="text-muted">@item.RemindInMinutes</small>
                                            </td>

                                            <td >
                                                <small class="text-muted">@item.User.FullName</small>
                                            </td>
                                            <td>
                                                <div class="item-action dropdown">
                                                    <a href="#" data-toggle="dropdown" class="text-muted">
                                                        <i data-feather="more-vertical"></i>
                                                    </a>
                                                    <div class="dropdown-menu dropdown-menu-right bg-black" role="menu">
                                                        <a class="dropdown-item" href="../../TimeTables/Details/@item.Id" id="viewInstitution">
                                                            View
                                                        </a>
                                                       @* <a class="dropdown-item edit" href="../../TGIFMatches/Edit/@item.Id" id="editInstitution">
                                                            Update
                                                        </a>
                                                       
                                                        <a class="dropdown-item edit" href="../../TGIFMatches/Delete/@item.Id" id="editInstitution">
                                                            Delete  
                                                        </a>*@
                                                    </div>
                                                </div>
                                            </td>
                                        </tr>
                                   }
                                    </tbody>

                                </table>
                                <div>

                                </div>


                              